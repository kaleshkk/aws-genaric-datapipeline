{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "The codepipeline for genaric pipeline",
    "Parameters": {
        "AWSCodePipelineServiceRole": {
            "Type": "String",
            "Description": "AWSCodePipelineServiceRole",
            "Default": "arn:aws:iam::680832645642:role/service-role/AWSCodePipelineServiceRole-us-east-1-test"
        },
        "ConnectionArn": {
            "Type": "String",
            "Description": "codestart connection ARN",
            "Default": "arn:aws:codestar-connections:us-east-1:680832645642:connection/0824f3ec-e9ed-416b-8560-86ab187deb2a"
        },
        "Repository": {
            "Type": "String",
            "Description": "Bitbucket code repository",
            "Default": "kaleshkk/aws-genaric-datapipeline"
        },
        "Branch": {
            "Type": "String",
            "Description": "Bitbucket branch",
            "Default": "without_cdk"
        },
        "BucketName": {
            "Type": "String",
            "Description": "S3 bucket for deploy",
            "Default": "genaric-pipeline-code-resourse"
        },
        "S3ObjectKey": {
            "Type": "String",
            "Description": "S3 location for deploy",
            "Default": "Deploy/aws-genaric-datapipeline"
        },
        "CloudformationRole": {
            "Type": "String",
            "Description": "Codebuild project name",
            "Default": "arn:aws:iam::680832645642:role/cloudformation-role"
        }
    },
    "Resources": {
        "DatahubAwsCD": {
            "Type": "AWS::CodePipeline::Pipeline",
            "Properties": {
                "Name": "generic-pipeline",
                "RoleArn": {
                    "Ref": "AWSCodePipelineServiceRole"
                },
                "ArtifactStore": {
                    "Type": "S3",
                    "Location": "genaric-codepipeline-bck"
                },
                "Stages": [
                    {
                        "Name": "Source",
                        "Actions": [
                            {
                                "Name": "source",
                                "ActionTypeId": {
                                    "Category": "Source",
                                    "Owner": "AWS",
                                    "Provider": "CodeStarSourceConnection",
                                    "Version": "1"
                                },
                                "Configuration": {
                                    "BranchName": {
                                        "Ref": "Branch"
                                    },
                                    "ConnectionArn": {
                                        "Ref": "ConnectionArn"
                                    },
                                    "FullRepositoryId": {
                                        "Ref": "Repository"
                                    },
                                    "OutputArtifactFormat": "CODE_ZIP"
                                },
                                "OutputArtifacts": [
                                    {
                                        "Name": "SourceArtifact"
                                    }
                                ],
                                "InputArtifacts": [],
                                "Region": "us-east-1"
                            }
                        ]
                    },
                    {
                        "Name": "Deploy",
                        "Actions": [
                            {
                                "Name": "Deploy",
                                "ActionTypeId": {
                                    "Category": "Deploy",
                                    "Owner": "AWS",
                                    "Provider": "S3",
                                    "Version": "1"
                                },
                                "Configuration": {
                                    "BucketName": {
                                        "Ref": "BucketName"
                                    },
                                    "Extract": "true",
                                    "ObjectKey": {
                                        "Ref": "S3ObjectKey"
                                    }
                                },
                                "OutputArtifacts": [],
                                "InputArtifacts": [
                                    {
                                        "Name": "OutputArtifact"
                                    }
                                ],
                                "Region": "us-east-1",
                                "Namespace": "DeployVariables"
                            },
                            {
                                "Name": "CUSTOMER_DEMAND_GENARIC_RESOURCES",
                                "ActionTypeId": {
                                    "Category": "Deploy",
                                    "Owner": "AWS",
                                    "Provider": "CloudFormation",
                                    "Version": "1"
                                },
                                "RunOrder": 2,
                                "Configuration": {
                                    "ActionMode": "CREATE_UPDATE",
                                    "RoleArn": {
                                        "Ref": "CloudformationRole"
                                    },
                                    "StackName": "DLake-Customer-Demand-Genaric-Resources",
                                    "TemplateConfiguration": "SourceArtifact::generic_resources/CloudFormation/Config/params.dev.json",
                                    "TemplatePath": "SourceArtifact::generic_resources/CloudFormation/genaric_resources_cloudformation.yml"
                                },
                                "OutputArtifacts": [],
                                "InputArtifacts": [
                                    {
                                        "Name": "SourceArtifact"
                                    }
                                ],
                                "Region": "us-east-1"
                            },
                            {
                                "Name": "DLake-Customer-Demand-SamplePipeline-Pipeline",
                                "ActionTypeId": {
                                    "Category": "Deploy",
                                    "Owner": "AWS",
                                    "Provider": "CloudFormation",
                                    "Version": "1"
                                },
                                "RunOrder": 2,
                                "Configuration": {
                                    "ActionMode": "CREATE_UPDATE",
                                    "RoleArn": {
                                        "Ref": "CloudformationRole"
                                    },
                                    "StackName": "DLake-Customer-Demand-SamplePipeline-Pipeline",
                                    "TemplateConfiguration": "SourceArtifact::pipelines/SamplePipeline/CloudFormation/Config/params.dev.json",
                                    "TemplatePath": "SourceArtifact::pipelines/SamplePipeline/CloudFormation/cloudformation.json"
                                },
                                "OutputArtifacts": [],
                                "InputArtifacts": [
                                    {
                                        "Name": "SourceArtifact"
                                    }
                                ],
                                "Region": "us-east-1"
                            }
                        ]
                    }
                ]
            }
        }
    }
}